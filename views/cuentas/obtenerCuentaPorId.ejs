<!-- views/obtenerCuentaPorId.ejs -->
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Detalles de Cuenta</title>
</head>
<body>
  <h1>Detalles de Cuenta</h1>
  <p><strong>ID:</strong> <%= cuenta.id %></p>
  <p><strong>Nombre:</strong> <%= cuenta.nombre %></p>
  <p><strong>Descripción:</strong> <%= cuenta.descripcion %></p>
  <p><strong>Serial:</strong> <%= cuenta.serial %></p>
  <p><strong>Fecha de Adquisición:</strong> <%= cuenta.fechaAdquisicion %></p>
  <p><strong>Estado:</strong> <%= cuenta.estado %></p>
</body>
</html>
